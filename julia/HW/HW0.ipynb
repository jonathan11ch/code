    "struct Perceptron{T}\n",
    "    W::Vector{T}\n",
    "    sigma\n",
    "end\n",
    "function (p::Perceptron)(x::Vector)\n",
    "    return p.sigma(p.W'*x) \n",
    "end\n",
    "\n",
    "mutable struct PerceptronOpt\n",
    "    eta::Real\n",
